<div class="row row-cards">
    <input type="hidden" id="sdate" value="<%= sdate %>" />
    <input type="hidden" id="edate" value="<%= edate %>" />
    <div class="col-lg-12 col-xl-12">
        <div class="card">
            <div class="card-header">
                <h3 class="card-title"><%= new Date(sdate).toDateString() %>  - <%= new Date(edate).toDateString() %></h3>
            </div>
            <div class="card-body e-table pt-1 px-5 pb-5">
                <input type="hidden" id="chosen_manager" />
                <div class="table-responsive table-lg pt-2">
                    <table class="table table-striped table-bordered text-nowrap w-100" id="logtable">
                        <thead>
                            <tr>
                                <th width="12%">User</th>
                                <th width="15%">Patient</th>
                                <th>Event</th>
                                <th width="7%">Action</th>
                                <th width="7%">Outcome</th>
                                <th width="12%">Date Time</th>
                            </tr>
                        </thead>
                        <tbody>
                            
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div><!-- COL-END -->
</div>


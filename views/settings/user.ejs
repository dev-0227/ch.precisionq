<div class="card">
  <div class="card-header border-0 pt-6">
    <div class="card-title">
      <!--begin::Search-->
      <div class="d-flex align-items-center position-relative my-1">
        <i class="ki-duotone ki-magnifier fs-3 position-absolute ms-5">
          <span class="path1"></span>
          <span class="path2"></span>
        </i>
        <input id="table_search_input"  type="text" data-kt-user-table-filter="search"  class="form-control form-control-solid w-250px ps-13"  placeholder="Search user" />
      </div>
      <!--end::Search-->
    </div>
    <div class="card-toolbar">
      <a class="btn btn-primary useraddbtn" id="add_btn" ><i class="fa fa-plus">&nbsp;</i>Add User</a>
    </div>
  </div>
  <div class="card-body py-4">
    <input type="hidden" id="chosen_user" />

    <table class="table table-striped table-bordered text-nowrap w-100"  id="usertable" >
      <thead>
        <tr>
          <th>Name</th>
          <th>Email</th>
          <th>Phone</th>
          <th>Role</th>
          <th>Active</th>
          <th class="text-center">Actions</th>
        </tr>
      </thead>
      <tbody></tbody>
    </table>
  </div>
</div>



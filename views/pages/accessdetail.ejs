    <div class="col-md-12">
        <div id="main">
            <div class="card">
                <div class="card-body">
                    <div class="table-responsive table-lg pt-2">
                        <table class="table table-striped table-bordered text-nowrap w-100" id="maintable">
                            <thead>
                                <tr>
                                    <th class="text-center">Sunday</th>
                                    <th class="text-center">Monday</th>
                                    <th class="text-center">Tuesday</th>
                                    <th class="text-center">Wednesday</th>
                                    <th class="text-center">Thursday</th>
                                    <th class="text-center">Friday</th>
                                    <th class="text-center">Saturday</th>
                                    <th class="text-center">Total</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>26</td>
                                    <td>26</td>
                                    <td>26</td>
                                    <td>26</td>
                                    <td>26</td>
                                    <td>26</td>
                                    <td>26</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
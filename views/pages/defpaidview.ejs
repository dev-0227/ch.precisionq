<div class="row ml-2 mr-2 mt-2">
    <div class="col-md-6">
        <h2 class="clinic-title text-primary"></h2>
	    <h4 class="text-primary clinic-addr"></h4>
	    <h5 class="text-primary paid-date"><span id="sdate"><%= sdate %></span> - <span id="edate"><%= edate %></span></h5>
	    <h5 class="text-primary paid-desc"></h5>
    </div>
    <div class="col-md-6">
        <div class="card">
            <div class="row">
                <div class="col-xl-3 col-lg-6 col-sm-6 pr-0 pl-0 border-right">
                    <div class="card-body text-center">
                        <h6 class="mb-0">Fee For Service Total</h6>
                        <h2 class="mb-1 mt-2 number-font text-primary"><span class="counter" id="totalamount"></span></h2>
                        <!-- <p class="mb-0 text-muted"> Completed</p> -->
                    </div>
                </div>
                <div class="col-xl-3 col-lg-6 col-sm-6 pr-0 pl-0 border-right">
                    <div class="card-body text-center">
                        <h6 class="mb-0">Numbers Of Visits</h6>
                        <h2 class="mb-1 mt-2 number-font text-secondary"><span class="counter" id="totalvisits"></span></h2>
                        <!-- <p class="mb-0 text-muted"> Completed</p> -->
                    </div>
                </div>
                <div class="col-xl-3 col-lg-6 col-sm-6 pr-0 pl-0 border-right">
                    <div class="card-body text-center">
                        <h6 class="mb-0">Unique Patients</h6>
                        <h2 class="mb-1 mt-2 number-font text-success"><span class="counter" id="totalpts"></span></h2>
                        <!-- <p class="mb-0 text-muted">Completed</p> -->
                    </div>
                </div>
                <div class="col-xl-3 col-lg-6 col-sm-6 pr-0 pl-0">
                    <div class="card-body text-center">
                        <h6 class="mb-0">Insurances</h6>
                        <h2 class="mb-1 mt-2 number-font text-danger"><span class="counter" id="totalins"></span></h2>
                        <!-- <p class="mb-0 text-muted">Completed</p> -->
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row ml-2 mr-2 mt-2">
    <div class="col-md-12">
        <h6 class="text-center text-muted">FFS Payments By Insurance</h6>
        <div id="amountbyins" style="width: 100%;height: 300px;"></div>
    </div>
    <div class="col-md-12 mt-4">
        <h6 class="text-center text-muted">Numbers of Visits and Unique Patients</h6>
        <div id="countbyvisitandpt" style="width: 100%;height: 300px;"></div>
    </div>
    <div class="col-md-12 mt-4">
        <div class="row" id="groupsbyinsarea"></div>
    </div>
    <div class="col-md-12 mt-4">
        <h6 class="text-center text-muted">Services Paid by Area</h6>
        <div id="groupamount" style="width: 100%;height: 300px;"></div>
    </div>
    <div class="col-md-12 mt-4">
        <div class="row" id="subgroupsbyinsarea"></div>
    </div>
    <div class="col-md-12 mt-4">
        <h6 class="text-center text-muted">FFS Best Paid Codes</h6>
        <div id="bestcptcode" style="width: 100%;height: 300px;"></div>
    </div>
</div>
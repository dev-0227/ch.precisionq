<div class="modal fade" id="appointment_modal" tabindex="-1" role="dialog"  aria-hidden="true">
	<div class="modal-dialog modal-fullscreen p-9" role="document">
		<div class="modal-content modal-rounded">
			<div class="modal-header">
				<div class="fs-1 fw-bold text-gray-900 my-3" >Appointments </div>
				<input type="hidden" id="appt_pt_mid" />
				
				<input type="hidden" id="appt_pt_inspcpid" />
				<input type="hidden" id="appt_pt_cyear" />
				<input type="hidden" id="appt_pt_emrid" />
				<input type="hidden" id="appt_pt_insurance" />
				<div class="btn btn-icon btn-sm btn-active-light-primary ms-2" data-bs-dismiss="modal" aria-label="Close">
					<i class="ki-duotone ki-cross fs-1"><span class="path1"></span><span class="path2"></span></i>
				</div>
			</div>
			<div class="modal-body">
				<div class="row h-100 flex-wrap align-content-start">
					<div class="d-flex  flex-sm-nowrap mb-3 col-md-12 mh-50px border-bottom">
						<div class="row  align-items-center mb-2 w-100">
							<div class="col-md-8 d-flex align-items-center">
								<div class="symbol symbol-40px symbol-circle me-1" >
									<span class="symbol-label fs-3 bg-primary text-inverse-primary fw-bold" id="appt_pt_name_icon">A</span>
								</div>
								<a class="text-gray-900 text-hover-primary fs-2 fw-bold mx-2" ><div class="" id="appt_pt_fullname"></div></a>
								<div class="fw-semibold fs-6 m-1 pe-2">
									<a  class="d-flex align-items-center text-gray-500 text-hover-primary me-5 ">
										<div class="symbol symbol-20px symbol-circle me-2" data-bs-toggle="tooltip"  data-kt-initialized="1">
											<span class="symbol-label bg-info text-inverse-info fw-bold">L</span>
										</div><div id="appt_pt_language"></div></a>
								</div>
								<div class= "d-flex align-items-center fw-semibold fs-6 m-1 pe-2">
									<a  class="d-flex align-items-center text-gray-500 text-hover-primary me-5 ">
										<div class="symbol symbol-20px symbol-circle me-2" data-bs-toggle="tooltip"  data-kt-initialized="1">
										<span class="symbol-label bg-primary text-inverse-primary fw-bold">S</span>
										</div><div class="" id="appt_pt_gender">Female</div></a>
								</div>
								<div class="d-flex align-items-center fw-semibold fs-6 m-1 pe-2">
									<div class=" fw-bold fs-6 pe-1 text-active-info ">DOB:</div>
									<div class="text-gray-500" id="appt_pt_dob"></div>
								</div>
								<div class= "d-flex align-items-center fw-semibold fs-6 m-1 pe-2">
									<div class=" fw-bold fs-6 pe-1 text-active-info ">Age:</div>
									<div class="text-gray-500" id="appt_pt_age"></div>
								</div>
								<div  class="mx-3 mt-1 cursor-pointer pt_info ms-20" >
									<i class="ki-outline ki-information-2 text-warning fs-2x">
									</i>
								</div>
								
							</div>
							<div class="col-md-4 d-flex justify-content-end">
								<a class="btn btn-primary permissionaddbtn" id="appt_add_btn" ><i class="fa fa-plus">&nbsp;</i>New</a>
							</div>
						</div>
					</div>
					<div class="d-flex flex-wrap flex-sm-nowrap mb-3 col-md-3" style="height: calc(100% - 50px);">
						<div class="d-flex align-content-between h-100 row w-100">
							<div class="d-flex justify-content-between align-items-start flex-wrap mb-2">
								<div class="d-flex flex-column w-100">
									<div class= "fw-semibold fs-6 m-1 pe-2">
										<a  class="d-flex align-items-center text-gray-500 text-hover-primary me-5 mb-2">
										<i class="ki-duotone ki-geolocation fs-1 me-1">
											<span class="path1"></span>
											<span class="path2"></span>
										</i><div class="" id="appt_pt_address"></div></a>
									</div>
									<div class= "fw-semibold fs-6 m-1 pe-2 d-flex align-items-center">
										<a  class="d-flex align-items-center text-gray-500 text-hover-primary me-5 mb-2">
											<i class="fa fa-phone ms-1 me-3 " aria-hidden="true"></i> <div id="appt_pt_telephone" class="calling cursor-pointer"></div></a>
									</div>
									<div class= "fw-semibold fs-6 m-1 pe-2 d-flex align-items-center">
										<a  class="d-flex align-items-center text-gray-500 text-hover-primary me-5 mb-2">
											<i class="ki-duotone ki-phone fs-1 me-1">
											<span class="path1"></span>
											<span class="path2"></span>
											</i> <div id="appt_pt_phone" class="calling cursor-pointer"></div></a>
									</div>
									<div class= "fw-semibold fs-6 m-1 pe-2">
										<a  class="d-flex align-items-center text-gray-500 text-hover-primary me-5 mb-2">
										<i class="ki-duotone ki-sms fs-1 me-1">
											<span class="path1"></span>
											<span class="path2"></span>
										</i><div id="appt_pt_email"></div></a>
									</div>
									<div class= "fw-semibold fs-6 m-1 pe-2">
										<a  class="d-flex align-items-center text-gray-500 text-hover-primary me-5 mb-2">
											<div class="symbol symbol-20px symbol-circle me-2" data-bs-toggle="tooltip"  data-kt-initialized="1">
												<span class="symbol-label bg-success text-inverse-success fw-bold">I</span>
											</div>Ins ID: <div id="appt_pt_insid"></div></a>
									</div>
									<div class= "fw-semibold fs-6 m-1 pe-2">
										<a  class="d-flex align-items-center text-gray-500 text-hover-primary me-5 mb-2">
											<div class="symbol symbol-20px symbol-circle me-2" data-bs-toggle="tooltip"  data-kt-initialized="1">
												<span class="symbol-label bg-info text-inverse-info fw-bold">M</span>
											</div>MRN: <div id="appt_pt_mrnid" class="px-2"></div></a>
									</div>
									<!--end::Info-->
								</div>
							</div>
							<div class="d-flex flex-wrap flex-stack m-4 pe-16">
								<div class="d-flex align-items-center w-100 flex-column mt-3">
									<div class="d-flex justify-content-between w-100 mt-auto mb-2">
										<span class="fw-semibold fs-6 text-gray-500">Completed</span>
										<span class="fw-bold fs-6">0%</span>
									</div>
									<div class="h-5px mx-3 w-100 bg-light mb-3">
										<div class="bg-success rounded h-5px" role="progressbar" style="width: 1%;" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100"></div>
									</div>
								</div>
							</div>
						</div>
					</div>
				  <div class="card-body col-md-9 ">
					<input type="hidden" id="chosen_vital" />
					
					<table class="table table-striped table-bordered text-nowrap w-100"  id="appointment_table" >
					  <thead>
						<tr >
							<th width="5%"></th>
							<th width="30%">Provider</th>
							<th width="30%">Reason</th>
							<th width="30%">Status</th>
							<th width="20%">Date Time</th>
							<th width="15%" class="text-center col-md-3"></th>
						</tr>
					  </thead>
					  <tbody></tbody>
					</table>
				  </div>
				</div>
			</div>
			
		</div>
	</div>
</div>

<div class="modal fade" id="appointment_edit_modal" tabindex="-1" role="dialog"  aria-hidden="true">
	<div class="modal-dialog modal-xl" role="document">
	  <div class="modal-content">
		<div class="modal-header pb-1 border-gray-400">
		  <div class="fs-1 fw-bold text-gray-900 my-3" id="appointment_title">Appointment </div>
		  <div class="btn btn-icon btn-sm btn-active-light-primary ms-2" data-bs-dismiss="modal" aria-label="Close">
			  <i class="ki-duotone ki-cross fs-1"><span class="path1"></span><span class="path2"></span></i>
		  </div>
		  
		</div>
		<div class="modal-body pt-4">
			<%- include("appointmentModalBody"); %>
		</div>
		
	  </div>
	</div>
</div>

<%- include("../patients/patientModal"); %>


<div class="modal fade" id="encounter_modal" tabindex="-1" role="dialog"  aria-hidden="true">
	<div class="modal-dialog modal-fullscreen p-9" role="document">
		<div class="modal-content modal-rounded">
			<div class="modal-header">
				<div class="fs-1 fw-bold text-gray-900 my-3" >Encounters </div>
				<input type="hidden" id="pt_mid" />
				<input type="hidden" id="pt_emrid" />
				<input type="hidden" id="pt_insurance" />
				<div class="btn btn-icon btn-sm btn-active-light-primary ms-2" data-bs-dismiss="modal" aria-label="Close">
					<i class="ki-duotone ki-cross fs-1"><span class="path1"></span><span class="path2"></span></i>
				</div>
			</div>
			<div class="modal-body">
				<div class="row h-100 flex-wrap align-content-start">
					<div class="d-flex  flex-sm-nowrap mb-3 col-md-12 mh-50px border-bottom">
						<div class="row  align-items-center mb-2 w-100">
							<div class="col-md-8 d-flex align-items-center">
								<div class="symbol symbol-40px symbol-circle me-1" >
									<span class="symbol-label fs-3 bg-primary text-inverse-primary fw-bold" id="pt_name_icon">A</span>
								</div>
								<a class="text-gray-900 text-hover-primary fs-2 fw-bold mx-2" ><div class="" id="pt_fullname"></div></a>
								<a  class="btn btn-sm btn-light-success fw-bold ms-2 fs-8 py-1 px-3" ></a>
								<div class="fw-semibold fs-6 m-1 pe-2">
									<a  class="d-flex align-items-center text-gray-500 text-hover-primary me-5 ">
										<div class="symbol symbol-20px symbol-circle me-2" data-bs-toggle="tooltip"  data-kt-initialized="1">
											<span class="symbol-label bg-info text-inverse-info fw-bold">L</span>
										</div><div id="pt_language"></div></a>
								</div>
								<div class= "d-flex align-items-center fw-semibold fs-6 m-1 pe-2">
									<a  class="d-flex align-items-center text-gray-500 text-hover-primary me-5 ">
										<div class="symbol symbol-20px symbol-circle me-2" data-bs-toggle="tooltip"  data-kt-initialized="1">
										<span class="symbol-label bg-primary text-inverse-primary fw-bold">S</span>
										</div><div class="" id="pt_gender">Female</div></a>
								</div>
